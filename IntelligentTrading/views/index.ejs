<% include partials/header %>
    <h1>Add Customer</h1>
    <% if(errors != undefined){ %>
        <ul>
            <% errors.forEach(function(error){ %>
            <li><%= error.msg %></li>
            <% }) %>
        </ul>
    <% } %>
    <form method="POST" action="/users/add">
        <label>First Name</label>
        <input type="text" name="first_name">
        <br>
        <br>
        <input type="submit" value="Submit">
    </form>
    <h1><%= title %></h1>
<% include partials/footer %>